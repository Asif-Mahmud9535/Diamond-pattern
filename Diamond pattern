#include<stdio.h>
main()
{
    int i,m,r,c,p=1,n;

    scanf("%d %d",&n,&m);
for(i=1;i<=m;i++)
{
    for(r=n-1; r>=1; r--)
    {
        for(c=1;c<=r;c++)
        {
            printf("*");
        }
        for(c=1; c<=n-r; c++)
        {
            printf("  ");
        }
        for(c=1;c<=r;c++)
        {
            printf("*");
        }
        printf("\n");
    }
    for(r=1; r<=n; r++)
    {
        for(c=1;c<=r;c++)
        {
            printf("*");
        }
        for(c=1; c<=n-r; c++)
        {
            printf("  ");
        }
        for(c=1;c<=r;c++)
        {
            printf("*");
        }
        printf("\n");
    }
}
}

